    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tiptap/extension-mention": "^2.27.1",
    "@tiptap/react": "^2.27.1",
    "@tiptap/starter-kit": "^2.27.1",
    "@tiptap/suggestion": "^2.27.1",
    "@vercel/blob": "^2.0.0",
    "@xyflow/react": "^12.9.3",
    "ai": "^5.0.100",
    "better-auth": "^1.3.34",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "dify-ai-provider": "^1.0.1",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.7",
    "emoji-picker-react": "^4.15.2",
    "framer-motion": "^12.23.24",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "ioredis": "^5.8.2",
    "json-schema": "^0.4.0",
    "katex": "^0.16.25",
    "lucide-react": "^0.486.0",
    "mermaid": "^11.12.1",
    "nanoid": "^5.1.6",
    "next": "16.0.4",
    "next-intl": "^4.5.5",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "ollama-ai-provider-v2": "^1.5.3",
    "pg": "^8.16.3",
    "qwen-ai-provider": "^0.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
