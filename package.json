    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,json,mjs,ts,tsx,css}": [
      "pnpm format",
      "pnpm lint:fix"
    ],
    "*.{yml,yaml}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "@swc/core"
    ]
  }
}
