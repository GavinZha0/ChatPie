    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@better-auth-kit/seed": "^1.0.12",
    "@biomejs/biome": "1.9.4",
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
